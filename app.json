{
    "expo": {
        "name": "PirateHein",
        "description": "AH Bonus React Native App",
        "slug": "piratehein",
        "privacy": "public",
        "sdkVersion": "36.0.0",
        "version": "1.0.0",
        "orientation": "portrait",
        "primaryColor": "#181818",
        "icon": "assets/icons/PirateHein.png",
        "loading": {
            "icon": "assets/icons/PirateHein.png",
            "hideExponentText": false
        },
        "splash": {
            "backgroundColor": "#181818",
            "resizeMode": "contain",
            "image": "assets/icons/PirateHein.png"
        },
        "packagerOpts": {
            "assetExts": ["ttf", "mp4", "otf", "xml"]
        },
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "app.pirate.S4D"
        },
        "android": {
            "package": "app.pirate.S4D"
        },
        "platforms": ["android", "ios"],
        "githubUrl": "https://github.com/stijndevries/jQuery-ICA"
    }
}
