{
  "expo": {
    "name": "PirateHein",
    "description": "AH Bonus React Native App",
    "slug": "piratehein",
    "privacy": "unlisted",
    "sdkVersion": "36.0.0",
    "version": "1.0.0",
    "orientation": "portrait",
    "primaryColor": "#FFEAD6",
    "icon": "assets/icons/PirateHein.png",
    "loading": {
      "icon": "assets/icons/PirateHeinSplash.png",
      "hideExponentText": false
    },
    "splash": {
      "backgroundColor": "#FFEAD6",
      "resizeMode": "contain",
      "image": "assets/icons/PirateHeinSplash.png"
    },
    "packagerOpts": {
      "assetExts": [
        "ttf",
        "mp4",
        "otf",
        "xml"
      ]
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "pirate.hein.bonus",
      "publishBundlePath": "ios/piratehein/Supporting/shell-app.bundle",
      "publishManifestPath": "ios/piratehein/Supporting/shell-app-manifest.json"
    },
    "platforms": [
      "android",
      "ios"
    ],
    "githubUrl": "https://github.com/JvdHout1011/pirate_heijn"
  }
}
